{
    "Resources": {
        "SlackChannelConfiguration": {
            "Type": "AWS::Chatbot::SlackChannelConfiguration",
            "Properties": {
                "ConfigurationName": "${ConfigurationNameParameter}",
                "IamRoleArn": "${IamRoleArnParameter}",
                "LoggingLevel": "${LoggingLevelParameter}",
                "SlackChannelId": "${SlackChannelIdParameter}",
                "SlackWorkspaceId": "${SlackWorkspaceIdParameter}",
                "SnsTopicArns": "${SnsTopicArnsParameter}"
            }
        }
    }
}